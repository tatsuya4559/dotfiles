{
  "var": {
    "prefix": "var",
    "description": "variable block",
    "body": [
      "variable \"$1\" {",
      "\ttype = $2",
      "}"
    ]
  },
  "res": {
    "prefix": "res",
    "description": "resource block",
    "body": [
      "resource \"$1\" \"$2\" {",
      "\t$3",
      "}"
    ]
  },
  "mod": {
    "prefix": "mod",
    "description": "module block",
    "body": [
      "module \"$1\" {",
      "\t$2",
      "}"
    ]
  },
  "out": {
    "prefix": "out",
    "description": "output",
    "body": [
      "output \"$1\" {",
      "\tvalue = $2",
      "}"
    ]
  },
  "loc": {
    "prefix": "loc",
    "description": "locals",
    "body": [
      "locals {",
      "\t$1",
      "}"
    ]
  },
  "provider": {
    "prefix": "provider",
    "description": "provider",
    "body": [
      "provider \"aws\" {",
      "\tregion  = $1",
      "\tprofile = $2",
      "\tassume_role {",
      "\t\trole_arn = $3",
      "\t}",
      "\tdefault_tags {",
      "\t\ttags = {",
      "\t\t\tEnvironment = $4",
      "\t\t\tService     = $5",
      "\t\t}",
      "\t}",
      "}"
    ]
  },
  "version": {
    "prefix": "version",
    "description": "version",
    "body": [
      "terraform {",
      "\trequired_version = \"$1\"",
      "\trequired_providers {",
      "\t\taws = {",
      "\t\t\tsource  = \"hashicorp/aws\"",
      "\t\t\tversion = \"$2\"",
      "\t\t}",
      "\t}",
      "}"
    ]
  },
  "backend": {
    "prefix": "backend",
    "description": "backend",
    "body": [
      "terraform {",
      "\tbackend \"s3\" {",
      "\t\tregion                  = \"$1\"",
      "\t\tencrypt                 = true",
      "\t\tbucket                  = \"$2\"",
      "\t\tkey                     = \"$3\"",
      "\t\tshared_credentials_file = \"$4\"",
      "\t\tprofile                 = \"$5\"",
      "\t}",
      "}"
    ]
  }
}
