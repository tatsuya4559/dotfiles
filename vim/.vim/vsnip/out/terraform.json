{
  "var": {
    "prefix": "var",
    "description": "variable block",
    "body": [
      "variable \"$1\" {",
      "\tdescription = \"$2\"",
      "\ttype = $3",
      "}"
    ]
  },
  "res": {
    "prefix": "res",
    "description": "resource block",
    "body": [
      "resource \"$1\" \"$2\" {",
      "\t$3",
      "}"
    ]
  },
  "data": {
    "prefix": "data",
    "description": "data source",
    "body": [
      "data \"$1\" \"$2\" {",
      "\t$3",
      "}"
    ]
  },
  "mod": {
    "prefix": "mod",
    "description": "module block",
    "body": [
      "module \"$1\" {",
      "\tsource = \"$2\"",
      "",
      "\t$3",
      "\tproviders = {",
      "\t\t$4",
      "\t}",
      "}"
    ]
  },
  "out": {
    "prefix": "out",
    "description": "output",
    "body": [
      "output \"$1\" {",
      "\tdescription = \"$2\"",
      "\tvalue = $3",
      "}"
    ]
  },
  "loc": {
    "prefix": "loc",
    "description": "locals",
    "body": [
      "locals {",
      "\t$1",
      "}"
    ]
  },
  "provider": {
    "prefix": "provider",
    "description": "provider",
    "body": [
      "provider \"aws\" {",
      "\tregion  = $1",
      "\tassume_role {",
      "\t\trole_arn = $3",
      "\t}",
      "\tdefault_tags {",
      "\t\ttags = {",
      "\t\t\tEnvironment = $4",
      "\t\t\tService     = $5",
      "\t\t}",
      "\t}",
      "}"
    ]
  },
  "version": {
    "prefix": "version",
    "description": "version",
    "body": [
      "terraform {",
      "\trequired_version = \"$1\"",
      "\trequired_providers {",
      "\t\taws = {",
      "\t\t\tsource  = \"hashicorp/aws\"",
      "\t\t\tversion = \"$2\"",
      "\t\t}",
      "\t}",
      "}"
    ]
  },
  "backend": {
    "prefix": "backend",
    "description": "backend",
    "body": [
      "terraform {",
      "\tbackend \"s3\" {",
      "\t\tregion                  = \"$1\"",
      "\t\tencrypt                 = true",
      "\t\tbucket                  = \"$2\"",
      "\t\tkey                     = \"$3\"",
      "\t}",
      "}"
    ]
  },
  "caller": {
    "prefix": "caller",
    "description": "aws caller identity",
    "body": [
      "data \"aws_caller_identity\" \"self\" {}"
    ]
  }
}
