{
  "help": {
    "prefix": "help",
    "description": "generate help",
    "body": [
      ".PHONY: help",
      "help: ## Display this help",
      "\t@grep -E '^[a-zA-Z_-]+:.*?## .*\\$\\$' \\$(MAKEFILE_LIST) | awk 'BEGIN {FS = \":.*?## \"}; {printf \"\\033[36m%-20s\\033[0m %s\\n\", \\$\\$1, \\$\\$2}'"
    ]
  },
  "ph": {
    "prefix": "ph",
    "description": "phony",
    "body": [
      ".PHONY: $1",
      "$1:",
      "\t$2"
    ]
  }
}
