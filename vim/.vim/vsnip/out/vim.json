{
  "fn": {
    "prefix": "fn",
    "body": [
      "function! $1($2) abort",
      "\t$3",
      "endfunction"
    ]
  },
  "def": {
    "prefix": "def",
    "description": "vim9 script function",
    "body": [
      "def $1($2): $3",
      "\t$4",
      "enddef"
    ]
  },
  "test": {
    "prefix": "test",
    "description": "test",
    "body": [
      "def Test$1(): void",
      "  const tests = [",
      "    {input: [''], want: true},",
      "  ]",
      "",
      "  for tt in tests",
      "    const got = call($1, tt.input)",
      "    const msg = printf(",
      "      '$1(\"%s\") want %s, but got %s',",
      "      tt.input, tt.want, got)",
      "    assert.equals(got, tt.want, msg)",
      "  endfor",
      "enddef",
      "suite.Test$1 = Test$1"
    ]
  }
}
