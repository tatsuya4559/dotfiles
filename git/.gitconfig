  email = tatsuya.k1029@gmail.com
[github]
  user = tatsuya4559
[core]
  editor = vim
[delta]
  plus-emph-style = "syntax darkgreen"
  plus-non-emph-style = "syntax olive"
  minus-emph-style = "syntax darkred"
  minus-non-emph-style = "syntax red"
  navigate = true
[diff]
  colorMoved = dimmed-zebra
[interactive]
  ; diffFilter = delta --color-only
[merge]
  tool = vimdiff
  ff = false
  conflictstyle = diff3
[fetch]
  prune = true
[pull]
  ff = only
[push]
  default = current
[grep]
  lineNumber = true
  column = false
  fallbackToNoIndex = true
[alias]
  st = status
  co = checkout
  ap = add -p
  cm = commit -v
  fe = fetch --all
  br = branch
  di = diff
  ds = diff --staged
  dt = difftool
  mt = mergetool
  sw = !git branch -vv | grep -v '^\\s*\\*' | fzf | awk '{print $1}' | xargs git switch
  tidy = !git branch -v | grep [gone] -F | awk '{print $1}' | xargs git branch -d
  pr = !gh pr create --web
  view = !gh repo view --web
[ghq]
  root = ~/git
